package Clases;

public class Config {
	private int precioPr, idPr;
	private String nombrePr;
	
	public Config(int idPr, String nombrePr, int precioPr) {
		this.precioPr = precioPr;
		this.idPr = idPr;
		this.nombrePr = nombrePr;
	}

	public int getPrecioPr() {
		return precioPr;
	}

	public void setPrecioPr(int precioPr) {
		this.precioPr = precioPr;
	}

	public int getIdPr() {
		return idPr;
	}

	public void setIdPr(int idPr) {
		this.idPr = idPr;
	}

	public String getNombrePr() {
		return nombrePr;
	}

	public void setNombrePr(String nombrePr) {
		this.nombrePr = nombrePr;
	}
	
	

	
	
	
}
